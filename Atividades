//ATV Num maior e posicao
Var
// Seção de Declarações das variáveis 
vet: vetor[1..5] de inteiro
cont1, cont2, num, posicao: inteiro
maior, troca: inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
escreval("Digite os valores das posições:")
para cont1 de 1 ate 5 faca
leia (vet[cont1])
fimpara


para cont1 de 1 ate 5 faca
se vet[cont1] > maior entao
maior <- vet[cont1]
posicao <- cont1
fimse
fimpara
escreval ("O maior valor é:", maior, "(E a posição que ele está é:)", posicao)



para cont1 de 1 ate 5 faca
para cont2 de 1 ate 5 faca
se vet[cont1] < vet[cont2] entao
troca <- vet[cont1]
vet[cont1] <- vet[cont2]
vet[cont2] <- troca
fimse
fimpara
fimpara
para cont1 de 1 ate 5 faca
escreval (vet[cont1])
fimpara

