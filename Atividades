//ATV MATRIZ NOTAS
Var
   // Seção de Declarações das variáveis
   aluno: vetor [1..10] de caracter
   notas: vetor [1..10, 1..5] de real
   nomealuno: caracter
   cont1, cont2: inteiro


Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   aluno[1] <- ("Maria")
   aluno[2] <- ("José")
   aluno[3] <- ("Diogo")
   aluno[4] <- ("Julio")
   aluno[5] <- ("Ana")
   aluno[6] <- ("Noemi")
   aluno[7] <- ("Tiago")
   aluno[8] <- ("João")
   aluno[9] <- ("Ricardo")
   aluno[10] <- ("Kleber")

   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 5 faca
         notas[cont1, cont2] <- randi(10)
      fimpara
      fimpara
   escreval ("Quem é você?")
   escreval ("Maria,José,Diogo,Julio, Ana, Noemi,Tiago,João,Ricardo ou Kleber")
   leia (nomealuno)

   para cont1 de 1 ate 10 faca
        se (nomealuno = aluno[cont1]) entao
           para cont2 de 1 ate 5 faca
                escreva (notas[ cont1, cont2])
           fimpara
           escreval(" ------ Essas são as sua notas!")
        fimse
   fimpara

Fimalgoritmo
....................................................................................
//ATV MATRIZ ALEATORIO
Var
   // Seção de Declarações das variáveis
   matriz1: vetor [1..10, 1..10] de inteiro
   matriz2: vetor [1..10, 1..10] de inteiro
   matriz3: vetor [1..10, 1..10] de inteiro
   cont1, cont2: inteiro
Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 10 faca
         matriz1[cont1, cont2] <- randi(100)
         matriz2[cont1 ,cont2] <- randi(100)
      fimpara
   fimpara
   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 10 faca
         matriz3[cont1, cont2] <- (matriz1[cont1, cont2] + matriz2[cont1, cont2])
      fimpara
   fimpara
   escreval ("Matriz 1: ")
   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 10 faca
         escreva (matriz1[cont1, cont2])
      fimpara
   fimpara
   escreval ("Matriz 2: ")
   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 10 faca
         escreva (matriz2[cont1, cont2])
      fimpara
   fimpara
   escreval ("A soma das duas matrizes são: ")
   para cont1 de 1 ate 10 faca
      para cont2 de 1 ate 10 faca
         escreva (matriz3[cont1, cont2])
      fimpara
   fimpara

Fimalgoritmo
...................................................................................
Var
   // Seção de Declarações das variáveis
   valor1, valor2: inteiro
   resultado: real
funcao calcula(v1, v2:real): real
inicio
   retorne (v1/v2)
fimfuncao
Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   escreval ("Informe o primeiro valor:")
   leia (valor1)
   escreval("Informe o segundo valor:")
   leia(valor2)
   resultado <- calcula(valor1, valor2)
   escreval ("O resultado da soma será:", resultado)



Fimalgoritmo
